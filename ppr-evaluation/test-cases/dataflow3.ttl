:input3a dn:hasCopy [
	dn:refactoredInto [ dn:remodelledTo [ 
		dn:hasSection [ dn:hasSelection [ 
			dn:hasPart :a ]]]];
] .

:output3 dn:isPortionOf [ dn:isCopyOf :a ] .

:outputNone dn:isSelectionOf [ dn:remodelledFrom :input3b ] .

